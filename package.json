{
    "name": "TelUI-core",
    "version": "0.0.1",
    "private": true,

    "dependencies": {
    },

    "devDependencies": {
        "lodash":"*"
    },

    "browserify":{
        "transform":["browserify-shim"]
    },

    "browser":{
        "jquery" : "./core/jquery.js",
        "jquery-ui" : "./core/jquery-ui.js",
        "angular" : "./core/angular.js"
    },

    "browserify-shim":{
        "jquery":"$",
        "jquery-ui": { "depends" : ["jquery"] },
        "angular": { "exports" : "angular", "depends" : ["jquery":"$"] }
    }
}
