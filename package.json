{
    "name": "TelUI-Core",
    "version": "0.0.3",
    "private": true,

    "dependencies": {
    },

    "devDependencies": {
        "lodash":"*"
    },

    "main":"./core.js",

    "browserify":{
        "transform":["browserify-shim"]
    },

    "browser":{
        "jquery" : "./core/jquery.js",
        "jquery-ui" : "./core/jquery-ui.js",
        "angular" : "./core/angular.js",
        "angular-animate": "./lib/angular/angular-animate.js",
    	"angular-cookies": "./lib/angular/angular-cookies.js",
    	"angular-placeholder-shim": "./lib/angular/angular-placeholder-shim.js",
    	"angular-resource": "./lib/angular/angular-resource.js",
        "angular-route": "./lib/angular/angular-route.js",
    	"angular-sanitize": "./lib/angular/angular-sanitize.js"
    },

    "browserify-shim":{
        "jquery":"$",
        "jquery-ui": { "depends" : ["jquery"] },
        "angular": { "exports" : "angular", "depends" : ["jquery:$"] },
        "angular-animate": { "depends" : ["angular"] },
    	"angular-cookies": { "depends" : ["angular"] },
    	"angular-placeholder-shim": { "depends" : ["angular"] },
    	"angular-resource": { "depends" : ["angular"] },
        "angular-route": { "depends" : ["angular"] },
    	"angular-sanitize": { "depends" : ["angular"] }
    }
}
